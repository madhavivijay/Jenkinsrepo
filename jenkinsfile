pipeline {
	agent any
	stages {
		stage('Build'){
			steps {
				sh '''
					#!/bin/bash
					pwd
					ls
					echo "This is an build stage"
				'''
			}
		}
		stage('Deploy'){
			steps {
				echo "This is an deploy stage"
			}
		}
		stage('Test'){
			steps {
				sh '''
					echo "This is an testing stage"
				'''
			}
		}
	}
}
