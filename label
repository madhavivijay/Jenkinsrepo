pipeline {
  agent none
  stages {
    stage ('build') { 
      agent { label 'cslave' }
      steps {
      sh '''
      #!/bin/bash
      pwd
      echo "this is a build stage"
     '''
   }
  }
    stage ('deploy') {
     agent { label 'master' }
     steps { 
     echo "this is a deploy stage"
   }
  }
 }
}
